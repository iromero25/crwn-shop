{
  "name": "crwn-shop",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "node build/server",
    "client": "react-scripts start",
    "build": "react-scripts build && yarn build:server",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently --kill-others-on-fail \"yarn build:server:watch\" \"yarn server\" \"yarn client\"",
    "server": "nodemon build/server",
    "build:server": "tsc -p tsconfig.server.json",
    "build:server:watch": "tsc --watch -p tsconfig.server.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "body-parser": "^1.19.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "firebase": "^9.6.6",
    "firebase-admin": "^10.0.2",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-stripe-checkout": "^2.6.3",
    "react-toastify": "^8.1.0",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.1",
    "stripe": "^8.195.0",
    "typescript": "^4.1.2",
    "uniqid": "^5.4.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react-router-dom": "^5.3.1",
    "@types/redux-logger": "^3.0.9",
    "@types/redux-mock-store": "^1.0.3",
    "@types/stripe": "^8.0.417",
    "@types/styled-components": "^5.1.15",
    "@types/uniqid": "^5.3.2",
    "concurrently": "^6.5.1",
    "nodemon": "^2.0.15",
    "redux-mock-store": "^1.5.4",
    "styled-components": "^5.3.3"
  }
}
